<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{HH:mm:ss.SSS} [%t]  %-5level %M: %logger{36} - %msg%n" />
		</Console>
	</appenders>
	<loggers>
		<root level="INFO">
			<appender-ref ref="Console" />
		</root>
		<logger name="com.itheima.shop.mapper" level="DEBUG">
		</logger>

		<logger name="test" level="DEBUG">
		</logger>
	</loggers>
</configuration>